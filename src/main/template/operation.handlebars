
  <ul class='operations' >
    <li class='{{method}} operation' id='{{parentId}}_{{nickname}}'>
      <div class='heading'>
        <h3>
          <span class='http_method'>
          <a href='#!/{{parentId}}/{{nickname}}' class="toggleOperation">{{method}}</a>
          </span>
          <span class='path'>
          <a href='#!/{{parentId}}/{{nickname}}' class="toggleOperation">{{path}}</a>
          </span>
        </h3>
        <ul class='options'>
          <li>
          <a href='#!/{{parentId}}/{{nickname}}' class="toggleOperation">{{{join summary}}}</a>
          </li>
        </ul>
      </div>
      <div class='content' id='{{parentId}}_{{nickname}}_content' style='display:none'>
        {{#if notes}}
        <p>{{{join notes}}}</p>
        {{/if}}
        {{#oauth}}
        <div class="auth">
        <span class="api-ic ic-error"></span>{{/oauth}}
        {{#each oauth}}
          <div id="api_information_panel" style="top: 526px; left: 776px; display: none;">
          {{#each this}}
            <div title='{{{join this.description}}}'>{{this.scope}}</div>
          {{/each}}
          </div>
        {{/each}}
        {{#oauth}}</div>{{/oauth}}
        {{#oauth}}
        <div class='access'>
          <span class="api-ic ic-off" title="click to authenticate"></span>
        </div>
        {{/oauth}}
        <form accept-charset='UTF-8' class='sandbox'>
          <div style='margin:0;padding:0;display:inline'></div>
          {{#if parameters}}
          <h4>Parameters</h4>
          <table class='fullwidth'>
          <thead>
            <tr>
            <th style="width: 100px; max-width: 100px">Parameter</th>
            <th style="width: 200px; max-width: 200px">Value</th>
            <th style="width: 300px; max-width: 300px">Description</th>
            <th style="width: 90px; max-width: 90px">Param Type</th>
            <th style="width: 220px; max-width: 220px">Data Type</th>
            </tr>
          </thead>
          <tbody class="operation-params">

          </tbody>
          </table>
          {{/if}}
        </form>
        {{#if type}}
          <h4>Response Class</h4>
          <p><span class="model-signature" /></p>
          <br/>
        {{/if}}
        {{#if responseMessages}}
        <div style='margin:0;padding:0;display:inline'></div>
        <h4>Response Messages</h4>
        <table class='fullwidth'>
          <thead>
          <tr>
            <th style="width: 60px; max-width: 60px">Status</th>
            <th style="width: 390px; max-width: 390px">Reason</th>
            <th style="width: 488px; max-width: 488px">Model</th>
          </tr>
          </thead>
          <tbody class="operation-status">

          </tbody>
        </table>
        {{/if}}
        <div class="response-content-type" />
        {{#if isReadOnly}}
        {{else}}
          <div class='sandbox_header'>
            <input class='submit' name='commit' type='button' value='Try it out!' />
            <a href='#' class='response_hider' style='display:none'>Hide Response</a>
            <img alt='Throbber' class='response_throbber' src='{{swaggerRoot}}/images/throbber.gif' style='display:none' />
          </div>
        {{/if}}
        <div class='response' style='display:none'>
          <h4>Request</h4>
          <table>
            <tr>
              <td width="10" />
              <td><br/>url</td>
              <td><div class='block request_url'></div></td>
            </tr>
            <tr>
              <td width="10" />
              <td><br/>headers</td>
              <td><div class='block request_headers'></div></td>
            </tr>
            <tr>
              <td width="10" />
              <td><br/>body</td>
              <td><div class='block request_body'></div></td>
            </tr>
          </table>
          <h4>Response</h4>
          <table>
            <tr>
              <td width="10" />
              <td><br/>code</td>
              <td><div class='block response_code'></div></td>
            </tr>
            <tr>
              <td width="10" />
              <td><br/>headers</td>
              <td><div class='block response_headers'></div></td>
            </tr>
            <tr>
              <td width="10" />
              <td><br/>body</td>
              <td><div class='block response_body'></div></td>
            </tr>
          </table>
        </div>
      </div>
    </li>
  </ul>
